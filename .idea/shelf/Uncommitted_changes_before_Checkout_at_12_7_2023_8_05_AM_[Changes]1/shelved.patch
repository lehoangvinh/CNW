+var Script = function () {
+
+// easy pie chart
+
+    $('.percentage').easyPieChart({
+        animate: 1000,
+        size: 135,
+        barColor:'#ff6c60'
+    });
+    $('.percentage-light').easyPieChart({
+        barColor: function(percent) {
+            percent /= 100;
