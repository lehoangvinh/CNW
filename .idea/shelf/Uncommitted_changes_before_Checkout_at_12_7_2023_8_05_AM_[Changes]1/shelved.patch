+  }
+  
+a.fc-event,
+.fc-event-draggable {
+  cursor: pointer;
+  }
+  
+a.fc-event {
+  text-decoration: none;
+  }
+  
+.fc-rtl .fc-event {
+  text-align: right;
+  }
+  
+.fc-event-skin {
